{
  "name": "ts-axios-qyq",
  "version": "1.0.2",
  "description": "",
  "keywords": [],
  "main": "dist/axios.umd.js",
  "module": "dist/axios.es5.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "qiyongqiang <15589092898@163.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Duck-77/ts-axios"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "dev": "node examples/server.js",
    "cors": "node examples/server2.js",
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts --configPlugin typescript && typedoc --out docs --theme default --entryPoints src/index.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "prepub": "npm run test:prod && npm run build",
    "pub": "sh release.sh"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "/test/.*\\.(test|spec)\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "src/**/*.{js,ts}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/boot.ts"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 150
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/estree": "^1.0.6",
    "@types/jasmine-ajax": "^3.3.5",
    "@types/jest": "^29.5.14",
    "@types/lodash.camelcase": "^4.3.9",
    "@types/node": "^22.8.6",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.16",
    "atob": "^2.1.2",
    "body-parser": "^1.20.3",
    "colors": "^1.3.2",
    "commitizen": "^4.3.1",
    "connect-multiparty": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^7.1.2",
    "cz-conventional-changelog": "^2.1.0",
    "express": "^4.21.1",
    "husky": "^1.0.1",
    "jasmine-ajax": "^4.0.0",
    "jasmine-core": "^5.4.0",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "lodash.camelcase": "^4.3.0",
    "multer": "^1.4.5-lts.1",
    "nprogress": "^0.2.0",
    "prettier": "^3.3.3",
    "prompt": "^1.0.0",
    "qs": "^6.13.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^4.24.3",
    "rollup-plugin-sourcemaps2": "^0.4.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "semantic-release": "^24.2.0",
    "shelljs": "^0.8.3",
    "style-loader": "^4.0.0",
    "travis-deploy-once": "^3.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-dev-middleware": "^7.4.2",
    "webpack-hot-middleware": "^2.26.1",
    "yargs": "^17.7.2",
    "yargs-parser": "^21.1.1"
  }
}
